<li class="card">
    <a href="" id="car-{{id}}">
        <div class="image">
            <img class="car-img" src="{{image}}" alt="{{make}} {{model}}">
            <div class="image-overlay">
                <img class="overlay-logo" src="{{overlay}}">
            </div>
        </div>
        <h1>{{make}} {{model}}</h1>
        <div class="name-year">
            <p>Year: {{year}}</p>
            <p>${{format_price price}}/day</p>
        </div>
    </a>
</li>