<li class="card">
    <img src="{{location.cars.[11].image}}" alt="{{location.cars.[11].make}} {{location.cars.[11].model}}">
    <h1>{{location.cars.[11].make}} {{location.cars.[11].model}}</h1>
    <div class="name-year">
        <p>{{location.cars.[11].year}}</p>
        <p>${{location.cars.[11].price}}/day</p>
        <button id="car-{{location.cars.[11].id}}" class="book-btn">Rent</button>
    </div>
</li>