<li class="card">
    <a href="" id="car-{{location.cars.[11].id}}">
        <div class="image">
            <img class="car-img" src="{{location.cars.[11].image}}"
                alt="{{location.cars.[11].make}} {{location.cars.[11].model}}">
            <div class="image-overlay">
                <img class="overlay-logo" src="{{location.cars.[11].overlay}}">
            </div>
        </div>
        <h1>{{location.cars.[11].make}} {{location.cars.[11].model}}</h1>
        <div class="name-year">
            <p>Year: {{location.cars.[11].year}}</p>
            <p>${{format_price location.cars.[11].price}}/day</p>
        </div>
    </a>
</li>